{
    "name": "nextjs-pico-app-template",
    "version": "1.0.1",
    "private": true,
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=10.0.0"
    },
    "scripts": {
        "dev": "next dev",
        "build": "eslint . && jest --passWithNoTests && next build",
        "start": "next start",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint . --fix",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf .next out coverage",
        "analyze": "ANALYZE=true npm run build",
        "migrate:all": "tsx scripts/migrate/migrate-all.ts",
        "migrate:sync": "tsx scripts/migrate/sync.ts",
        "migrate:verify": "tsx scripts/migrate/verify.ts",
        "migrate:status": "tsx scripts/migrate/status.ts",
        "migrate:rollback": "tsx scripts/migrate/rollback.ts",
        "migrate:study-areas": "tsx scripts/migrate/study-areas.ts",
        "migrate:extracts": "tsx scripts/migrate/extracts.ts",
        "migrate:inline-images": "tsx scripts/migrate/inline-images.ts",
        "migrate:questions": "tsx scripts/migrate/questions.ts",
        "migrate:ddi-options": "tsx scripts/migrate/ddi-options.ts",
        "migrate:products": "tsx scripts/migrate/products.ts",
        "migrate:users": "tsx scripts/migrate/users.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.982.0",
        "@next/bundle-analyzer": "^16.1.1",
        "@picocss/pico": "^2.1.1",
        "@prisma/client": "^6.19.2",
        "@types/bcrypt": "^6.0.0",
        "bcrypt": "^6.0.0",
        "bcryptjs": "^2.4.3",
        "contentful": "^11.10.3",
        "dotenv": "^17.2.3",
        "mysql2": "^3.11.5",
        "next": "^16.1.1",
        "prisma": "^6.19.2",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "sharp": "^0.34.5",
        "swr": "^2.3.8",
        "tsx": "^4.21.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.35.0",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/jest": "^30.0.0",
        "@types/node": "^22.18.6",
        "@types/react": "19.1.13",
        "@types/react-dom": "19.1.9",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "@typescript-eslint/parser": "^8.44.0",
        "eslint": "^9.35.0",
        "eslint-config-next": "^16.1.1",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^16.4.0",
        "jest": "^30.1.3",
        "jest-environment-jsdom": "^30.1.2",
        "jiti": "^2.5.1",
        "next-router-mock": "^1.0.2",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.44.0"
    }
